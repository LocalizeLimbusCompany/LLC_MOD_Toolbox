{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/ProjektMing/LLC_MOD_Toolbox/refactor/schema.json",
    "contentEncoding": "utf-8",
    "title": "LLC_MOD_Toolbox_Node_List",
    "description": "https://www.zeroasso.top/docs/dev/toolbox/mynode",
    "properties": {
        "downloadNode": {
            "type": "array",
            "description": "下载节点",
            "items": {
                "$ref": "#/definitions/node"
            }
        },
        "apiNode": {
            "type": "array",
            "description": "API节点",
            "items": {
                "$ref": "#/definitions/node"
            }
        }
    },
    "required": [
        "downloadNode",
        "apiNode"
    ],
    "definitions": {
        "node": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "description": "节点名称"
                },
                "endpoint": {
                    "type": "string",
                    "format": "uri",
                    "description": "节点地址"
                },
                "isDefault": {
                    "type": "boolean",
                    "description": "是否为默认节点"
                }
            },
            "required": [
                "name",
                "endpoint"
            ],
            "additionalProperties": false
        }
    }
}